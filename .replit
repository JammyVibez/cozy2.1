modules = ["nodejs-20", "web"]
run = "npm run dev"
[agent]
expertMode = true
integrations = ["javascript_stripe==1.0.0==1.0.0", "javascript_supabase==1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
